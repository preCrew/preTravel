{
  "compilerOptions": {
    "target": "es2015",
    "strict": true,
    "lib":[
        "ES5",
        "ES2015",
        "DOM"//web 사용
    ],
    "forceConsistentCasingInFileNames":false,
    "moduleResolution": "node",
    "jsx":"react-jsx",   
    "esModuleInterop": true ,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
    },
    "jsxImportSource": "@emotion/react" // Add the css prop
  },
  "include": ["src", "@types"],

}